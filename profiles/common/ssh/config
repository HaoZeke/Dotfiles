Host *
  ForwardAgent yes
  ForwardX11 yes
  ForwardX11Trusted yes
  XAuthLocation __[xauth_loc]__

Host github.com
  IdentitiesOnly yes
  IdentityFile ~/.ssh/__[github_id]__

Host gitlab.com
  IdentitiesOnly yes
  IdentityFile ~/.ssh/__[gitlab_id]__

Host krafla
  Hostname krafla.rhi.hi.is
  User __[ui_id]__
  IdentitiesOnly yes
  IdentityFile ~/.ssh/id_rsa

Host garpur
  Hostname garpur.ihpc.hi.is
  User __[ui_id]__
  IdentitiesOnly yes
  IdentityFile ~/.ssh/garpur
  ProxyJump krafla

Host elja
  Hostname elja.hi.is
  User __[ui_id]__
  IdentitiesOnly yes
  IdentityFile ~/.ssh/elja
  ProxyJump krafla
